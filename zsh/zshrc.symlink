#!/bin/zsh

zstyle ':zdotfiles:load' dotmods \
  'directory' \
  'environment' \
  'git' \
  'go' \
  'puppet' \
  'python' \
  'ruby' \
  'utility' \
  'window'

if [ -s "$HOME/dotfiles/zsh/init.zsh" ]; then
  source "$HOME/dotfiles/zsh/init.zsh"
fi

